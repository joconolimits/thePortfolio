<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    @*@Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")*@

    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.4.0/css/font-awesome.min.css">
    <!-- main style -->
    <link rel="stylesheet" type="text/css" href="~/Content/style.css" media="screen" />

    <!-- color scheme -->
    <link rel="stylesheet" type="text/css" href="~/Content/orange.css" media="screen" />

    <!--miscellaneous-->
    <link rel="stylesheet" type="text/css" href="~/Content/custom-styles.css" media="screen" />
    <link rel="stylesheet" href="~/Content/template-changer.css" type="text/css" />

    <!-- revolution slider settings -->
    <link rel="stylesheet" type="text/css" href="~/rs-plugin/css/settings.css" media="screen" />

    <!-- responsive stylesheet -->
    <link rel="stylesheet" type="text/css" href="~/Content/responsive.css" media="screen" />

    <!-- Google Web Fonts -->
    <link href='http://fonts.googleapis.com/css?family=Open+Sans:300,400,600,700,800' rel='stylesheet' type='text/css'>

    <!-- jQuery -->
    <script type="text/javascript" src="http://ajax.googleapis.com/ajax/libs/jquery/1.10.2/jquery.min.js"></script>

    <!-- jQuery Scripts -->
    <script type="text/javascript" src="~/Scripts/frontEnd/scripts-top.js"></script>

    <script type="text/javascript" src="~/Scripts/frontEnd/template-changer.js"></script>
</head>
<body>
    
        @RenderBody()
     

    @*@Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)*@

    <script type="text/javascript" src="~/twitter/jquery.tweet.min.js"></script>
    <script type="text/javascript" src="~/Scripts/frontEnd/jquery.contact.js"></script>

    <script type="text/javascript" src="~/Scripts/frontEnd/scripts-bottom.js"></script>


    <!-- ISOTOPE -->
    <script type="text/javascript" src="~/Scripts/frontEnd/jquery.isotope.min.js"></script>
    <script type="text/javascript" src="~/Scripts/frontEnd/custom-isotope.js"></script>

    <!-- REVOLUTION SLIDER -->
    <script type="text/javascript" src="~/rs-plugin/js/jquery.themepunch.plugins.min.js"></script>
    <script type="text/javascript" src="~/rs-plugin/js/jquery.themepunch.revolution.min.js"></script>
    <script type="text/javascript" src="~/Scripts/frontEnd/custom-revolution.js"></script>

    <!-- GOOGLE MAPS -->
    <script type="text/javascript" src="http://maps.google.com/maps/api/js?sensor=false"></script>
    <script type="text/javascript" src="~/Scripts/frontEnd/custom-map.js"></script>

    <!-- CUSTOM -->
    <script type="text/javascript" src="~/Scripts/frontEnd/custom.js"></script>
</body>
</html>
